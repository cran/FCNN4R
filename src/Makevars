PKG_LIBS = `$(R_HOME)/bin/Rscript -e "Rcpp:::LdFlags()"` $(LAPACK_LIBS) $(BLAS_LIBS) $(FLIBS) -lstdc++
PREFIX =.
INCLUDE_FCNN_R = -I$(PREFIX)
OBJECTS_FCNN_R = $(PREFIX)/interface.o $(PREFIX)/fcnn/utils.o $(PREFIX)/fcnn/struct.o $(PREFIX)/fcnn/export.o $(PREFIX)/fcnn/report.o $(PREFIX)/fcnn/level2.o $(PREFIX)/fcnn/level3.o
PKG_CPPFLAGS = $(INCLUDE_FCNN_R) `$(R_HOME)/bin/Rscript -e "Rcpp:::CxxFlags()"`
OBJECTS = $(OBJECTS_FCNN_R)
PKG_CXXFLAGS = -DR_DLL -DFCNN_DOUBLE_ONLY
